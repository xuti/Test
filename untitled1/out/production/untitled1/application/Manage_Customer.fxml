<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Manage_Customer">
   <children>
      <Button layoutX="30.0" layoutY="225.0" minHeight="50.0" minWidth="100.0" mnemonicParsing="false" text="구매 결의서" />
